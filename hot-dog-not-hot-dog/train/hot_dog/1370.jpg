                    // Reset the status message
                    // Clear out the status message
                    $('.loginwith #status').html('');

                    FB.login(function(response) {
                        if (response.status === 'connected') {
                            response.requestType = 'request_log_in';
                            $.ajax({
                                url: '?__call=FacebookSSO',
                                type: 'post',
                                data: response,
                                success: function(results) {
                                    if (results && results.status) {
                                        if(results.newMember) {
                                            jQuery("#signup-form").modal('trackGA', 'signup success facebook', 'Registered', 'facebook');
                                        } else {
                                            jQuery("#signup-form").modal('trackGA', 'login success facebook', 'Logged In', 'facebook');
                                        }

                                        if (results.newMember) {
                                            window.additional_info_modal.modal('show');
                                            $(document).trigger('additionalInfoModalInit');
                                        } else {
                                            // show loading...
                                            jQuery("#signup-form").modal('showOverlay');
                                            window.location = window.location;
                                        }
                                    } else {
                                        if (results.message !== '') {
                                            $('.loginwith #status').html(results.message);
                                        } else {
                                            $('.loginwith #status').html('Sorry, there was an error trying to log you in.');
                                        }
                                    }
                                },
                                error: function(e) {
                                    $('.loginwith #status').html('Sorry, there was an error trying to log you in.');
                                }
                            });
                        } else if (response.status === 'not_authorized') {
                            $('.loginwith #status').html('Please authorize Education.com to log in.');
                        } else {
                            $('.loginwith #status').html('Sorry, there was an error trying to log you in.');
                        }
                    }, {
                                                scope: 'public_profile,email',
                                                auth_type: 'rerequest',
                                                return_scopes: true
                    });
                });
            };

            /** Events for the additional info request modal */
            $('.modal#additional-info-request').on('click', 'a#submit-additional-info', function() {
                // show loading...
                jQuery("#signup-form").modal('showOverlay');
                $.ajax({
                    url: '?__call=FacebookSSO',
                    type: 'post',
                    data: {
                        requestType: 'additional_info',
                        formData: JSON.stringify($(".modal#additional-info-request form").serializeArray())
                    },
                    complete: function () {
                        window.location = window.location;
                    }
                });

                $(document).trigger('saveSchoolInfo');
            });
            $('.modal#additional-info-request').on('click', 'a#skip-additional-info', function() {
                // show loading...
                jQuery("#signup-form").modal('showOverlay');
                window.location = window.location;
            });

            // Load the SDK asynchronously
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        })(jQuery);

    </script>

         

	
