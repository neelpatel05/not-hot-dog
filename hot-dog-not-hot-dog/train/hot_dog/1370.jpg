<script>
    jQuery(document).ready(function () {
        jQuery.ajaxSetup({cache:true});
        jQuery.getScript( window.Edu.prefixCDN('/js/typeahead.bundle.min.js?cb='+(new Date()).getDay()+'-'+(new Date()).getMonth()+'-'+(new Date()).getFullYear()), function() {

            function SchoolInfo (selector) {
                this.selector = selector;
                this.schoolNameInput = this.selector.find('.school-name-input[name="schoolName"]');
                this.addAnotherSchoolButton = this.selector.find('.add-another-school');
                this.removeInputButton = this.selector.find('.remove-input');
                this.addAnotherSchoolContainerToClone = this.selector.find('.school-name-hidden');
                this.hiddenClassName = 'school-name-hidden';
                this.schoolNameClassName = 'school-name';
                this.removeInputClassName = 'remove-input';
                this.schoolNameInputClassName = 'school-name-input';
                this.typeAheadClassName = 'typeahead';
                this.newSchoolCounterData = 0;

                if (this.schoolName) {
                    this.schoolNameInput.val(this.schoolName);
                }

                this.sendSchoolRequest = function () {
                    if (this.schoolNameInput.val() !== this.schoolName) {
                        this.schoolId = 0;
                    }

                    var schools = [];
                    jQuery('.school-name-input.tt-input').each(function () {
                        if (jQuery(this).val()) {
                            var school = {};
                            school.id = jQuery(this).attr('data-school-id');
                            school.name = jQuery(this).attr('data-school-name');
                            school.input = jQuery(this).val();
                            schools.push(school);
                        }
                    });

                    jQuery.ajax({
                        url: '/api/member/update-schools/',
                        type: 'post',
                        data: {
                            schools: schools
                        }
                    }).done(
                        function () {
                            window.location = jQuery('.modal#school-info').data('modal').redirectPathOnSuccess;
                        }
                    );
                };

                this.applyTypeAhead = function () {
                    this.selector.find('.'+this.typeAheadClassName).typeahead('destroy','NoCached').typeahead({
                        minLength: 2,
                        highlight: true
                    }, {
                        name: 'schools',
                        source: new Bloodhound({
                            datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name'),
                            queryTokenizer: Bloodhound.tokenizers.whitespace,
                            remote: {
                                url: '/api/schools/names/%QUERY%',
                                wildcard: '%QUERY%',
                                filter: function (data) {
                                    if (data) {
                                        return jQuery.map(data, function (object) {
                                            return {id: object.id, name: object.name};
                                        });
                                    } else {
                                        return {};
                                    }
                                }
                            }
                        }),
                        limit: 'Infinity',
                        display: 'name'
                    }).bind('typeahead:select', function(e, suggestion) {
                        jQuery(e.currentTarget).attr('data-school-id', suggestion.id).attr('data-school-name', suggestion.name);
                    });
                };

                this.applyTypeAhead();
                this.removeInput = function() {

                };
                this.addAnotherSchool = function () {
                    var clonedElement = this.addAnotherSchoolContainerToClone.clone();
                    clonedElement.removeClass(this.hiddenClassName).addClass(this.schoolNameClassName);
                    clonedElement.find('.'+this.schoolNameInputClassName).addClass(this.typeAheadClassName);
                    clonedElement.find('.remove-input').attr('data-input-id', this.newSchoolCounterData);
                    clonedElement.attr('data-input-id', this.newSchoolCounterData);
                    this.newSchoolCounterData++
                    this.selector.find('.input-row.school-name:last').after(clonedElement[0]);
                    this.applyTypeAhead();
                };
            }

            var schoolInfoModal = new SchoolInfo(jQuery('.modal#school-info'));
            // window.redirectPathOnSuccess = window.location.href; // i dont like this on the window but is a hot fix for the bundle
            schoolInfoModal.selector.modal({
                redirectPathOnSuccess: window.location.href,
                onOpen: function() {
                    //Refresh the page if user closes the modal
                    jQuery(document).on('modalClosed', function () {
                        window.location = jQuery('.modal#school-info').data('modal').redirectPathOnSuccess;
                    });
                },
                setRedirectOverride: function(path) {
                    // window.redirectPathOnSuccess = path;
                    this.redirectPathOnSuccess = path;
                },
                init: function() {
                    this.setListeners();
                },
                setListeners: function() {
                    var self = this;
                    schoolInfoModal.selector.on('click', 'a#skip-school-name', function() {
                        window.location = self.redirectPathOnSuccess;
                        schoolInfoModal.selector.modal('hide');
                    });

                }
            });

            jQuery('.school-info-button').on('click', function () {
                if(window.Edu && window.Edu.trackEvent)
                    window.Edu.trackEvent("Opened School Info", {});
                schoolInfoModal.selector.modal('show');

            });

            schoolInfoModal.addAnotherSchoolButton.on('click', function () {
                schoolInfoModal.addAnotherSchool();
            });
            schoolInfoModal.selector.on("click", '.remove-input', function (e) {
                $(e.target).closest('.input-row').remove();
            });
            schoolInfoModal.selector.on('click', 'a#submit-school-name', function() {
              if (window.Edu && window.Edu.trackEvent)
                window.Edu.trackEvent("Saved School Info", {});
              schoolInfoModal.selector.modal('hide');
              schoolInfoModal.sendSchoolRequest();
            });
            // }).on('click', 'a#skip-school-name', function() {
            //     schoolInfoModal.selector.modal('hide');
            //     window.location = window.redirectPathOnSuccess;
            // });

            jQuery(document).on('openSchoolInfoModal', function () {
                schoolInfoModal.selector.modal('show');

            });

            jQuery(document).on('saveSchoolInfo', function () {
                schoolInfoModal.sendSchoolRequest();


            });

            jQuery(document).on('additionalInfoModalInit', function () {
                var additionalInfoSchoolModal = new SchoolInfo(jQuery('.modal#additional-info-request'));
            });
        });
    });
</script>

