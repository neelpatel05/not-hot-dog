<script type='text/javascript'>
    var PREBID_TIMEOUT = 700;
    nextSlotId = 9;

    /**
     * Helper function for dynamic ads that generates slot names on the fly
     * @returns {number}
     */
    function generateNextSlotName() {
        var id = nextSlotId++;
        return id;
    }

    /**
     *  Refresh an ad by its unit path
     */
    function refreshAd( slotNumber, adUnitPath ) {
		// console.info('NYOVMG-ADS: Refreshing Ad', slotNumber, adUnitPath);
        if ( ! gptAdSlots[slotNumber] ) {
			// console.info('NYOVMG-ADS: Ad Slot Not Found', slotNumber, gptAdSlots);
            return;
        }

        // Refresh the bid for that ad slot
        var adSlot = gptAdSlots[slotNumber];
        refreshBid( adSlot.getSlotElementId(), adSlot );
    }

    /**
     * Refresh Bids
     * @param adSlot
     * @param adUnit
     */
    function refreshBid(adSlot, adUnitName) {
		// console.info('NYOVMG-ADS: Refreshing Bid', adSlot, adUnit);
        pbjs.que.push(function () {
            pbjs.requestBids({
                timeout: PREBID_TIMEOUT,
                adUnitCodes: [adSlot],
                bidsBackHandler: function () {
					// console.info('NYOVMG-ADS: bid back handler triggered');
                    pbjs.setTargetingForGPTAsync([adSlot]);
                    googletag.pubads().refresh([adUnitName], { changeCorrelator: false });
                }
            });
        });
    }

    /**
     * Dynamically create an ad
     *
     * @param adUnitPath
     * @param div
     * @param size_mapping
     * @param refresh
     */
    function useReserveSlot( adUnitPath, div, size_mapping, refresh = true, adSlotNumber ) {
        var adSlot = generateNextSlotName(); // @NOTE - this is generated and then thrown away?
        adSlot = googletag.defineSlot(adUnitPath, [], div).defineSizeMapping(size_mapping).setCollapseEmptyDiv(true).addService(googletag.pubads());

        googletag.display(div);

        if ( refresh ) {
            refreshBid(adSlot.getSlotElementId(), adSlot);
        }

        // Store the ad slot in gptAdSlots if required
        if ( adSlotNumber && ( ! gptAdSlots[adSlotNumber]) ) {
            gptAdSlots[adSlotNumber] = adSlot;
        }
    };

    /**
     * Dummy Timmer that refreshes two ads every 20seconds
     */
    window.ad_refresh_timeout = 20000;
	// window.ad_refresh_timeout = 5000; // Debug testing

    function canRefreshFixedArticle() {
        // The page must be in focus & the user not "inactive"
        if ( ! window.page_is_focused || window.user_is_inactive ) {
            return false;
        }

        // The browser must be >= 770px wide
        var windowWidth = jQuery( window ).width();
        if ( windowWidth < 770 ) {
            return false;
        }

        // All good
        return true;
    }

    function refreshAds() {
		// Refresh the ads that need it
		// console.info('NYOVMG-ADS: Refreshing Ads');
        googletag.cmd.push(function() {
            				// console.info('NYOVMG-ADS: pushing refresh for single article');
				if ( canRefreshFixedArticle() ) {
					// console.info('NYOVMG-ADS: pushing refresh for single fixed article');
					refreshAd( 3, '/1008049/observer_fixedarticle_1' );
				}
            
                    });
    }

    jQuery( document ).ready(function() {
        // Setup the repeating refresh
		// console.info('NYOVMG-ADS: Setting Up Interval', ad_refresh_timeout);
        setInterval(refreshAds, ad_refresh_timeout);
    });
</script>


